; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="tiny-creatures"
run/main_scene="res://main_menu.tscn"
config/features=PackedStringArray("4.4", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"
Game="*res://scenes/game.gd"

[dialogic]

directories/dch_directory={
"dave": "res://data/characters/dave.dch",
"joaquim": "res://data/characters/joaquim.dch",
"lilith": "res://data/characters/lilith.dch",
"me": "res://data/characters/me.dch",
"mickey": "res://data/characters/mickey.dch",
"tom": "res://data/characters/tom.dch"
}
directories/dtl_directory={
"chapter1_happy_birthday": "res://data/characters/chapter1_happy_birthday.dtl",
"test": "res://data/characters/test.dtl"
}
layout/style_list=["res://data/dialogic_bubble_style.tres", "res://data/dialogic_default_bubbles.tres", "res://data/dialogic_vn.tres"]
layout/default_style="res://data/dialogic_bubble_style.tres"
extensions_folder="res://addons/dialogic_additions"
text/letter_speed=0.01
text/initial_text_reveal_skippable=true
text/text_reveal_skip_delay=0.1
text/advance_delay=0.1
text/autoadvance_per_character_delay=0.1
text/autoadvance_ignored_characters_enabled=true
animations/join_default_length=0.5
animations/join_default_wait=true
animations/leave_default_length=0.5
animations/leave_default_wait=true
animations/cross_fade_default_length=0.5
save/autosave_delay=60.0
save/encryption_on_exports_only=true
choices/autofocus_first=true
choices/delay=0.2
choices/reveal_delay=0
choices/reveal_by_input=false
choices/hotkey_behaviour=0
text/autopauses={}

[display]

mouse_cursor/custom_image="res://assets/textures/hand-point-up-solid.png"

[editor]

naming/node_name_casing=2
export/convert_text_resources_to_binary=false

[editor_plugins]

enabled=PackedStringArray("res://addons/blender_godot_pipeline/plugin.cfg", "res://addons/dialogic/plugin.cfg")

